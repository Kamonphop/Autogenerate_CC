<init>{
	entry ->   
	MC: public class Client -> <init>;
	CF: public class Client -> exit;
}
main{
	entry ->   
	MC: Scanner in = getScanner(); -> getScanner;
	CF: Scanner in = getScanner(); -> int sizeOfArray = getSizeOfArray(in);;
	MC: int sizeOfArray = getSizeOfArray(in); -> getSizeOfArray;
	CF: int sizeOfArray = getSizeOfArray(in); -> int array[]=new int[sizeOfArray];;
	CF: int array[]=new int[sizeOfArray]; -> getElementsOfArray(in, sizeOfArray, array);;
	MC: getElementsOfArray(in, sizeOfArray, array); -> getElementsOfArray;
	CF: getElementsOfArray(in, sizeOfArray, array); -> Caller caller = new Caller(new unitdigitsort(sizeOfArray ,array));;
	MC: Caller caller = new Caller(new unitdigitsort(sizeOfArray ,array)); -> <init>;
	MC: Caller caller = new Caller(new unitdigitsort(sizeOfArray ,array)); -> <init>;
	CF: Caller caller = new Caller(new unitdigitsort(sizeOfArray ,array)); -> caller.arraysort();;
	MC: caller.arraysort(); -> arraysort;
	CF: caller.arraysort(); -> in.close();;
	MC: in.close(); -> close;
	CF: in.close(); -> };
	CF: } -> exit;
}
getElementsOfArray{
	entry ->   
	MC: System.out.println ("enter the elements of the array"); -> println;
	CF: System.out.println ("enter the elements of the array"); -> for(int i=0;i<sizeOfArray;i++);
	CF: for(int i=0;i<sizeOfArray;i++) -> array[i]=in.nextInt();;
	MC: array[i]=in.nextInt(); -> nextInt;
	CF: array[i]=in.nextInt(); -> for(int i=0;i<sizeOfArray;i++);
	CF: for(int i=0;i<sizeOfArray;i++) -> };
	CF: } -> exit;
}
getSizeOfArray{
	entry ->   
	MC: System.out.println ("Enter the no of elements of the array"); -> println;
	CF: System.out.println ("Enter the no of elements of the array"); -> int sizeOfArray = in.nextInt();;
	MC: int sizeOfArray = in.nextInt(); -> nextInt;
	CF: int sizeOfArray = in.nextInt(); -> return sizeOfArray;;
	CF: return sizeOfArray; -> exit;
}
getScanner{
	entry ->   
	MC: return new Scanner(System.in); -> <init>;
	CF: return new Scanner(System.in); -> exit;
}
